export TODOTXT_DEFAULT_ACTION=ls

# Some aliases
alias  t="todo.sh"
alias ta="todo.sh add"
alias tp="todo.sh pri"
alias td="todo.sh do"
alias tl="todo.sh ls"
alias tx="todo.sh addx"
alias te="mvim $T/todo.txt"
alias ted="mvim $T/done.txt"
alias tla="todo.sh listpri A"
alias tlb="todo.sh listpri B"
alias tlc="todo.sh listpri C"
alias tld="todo.sh listpri D"
alias tle="todo.sh listpri E"
alias tlnoc="t view context Nocosium"
alias tldoc="t view context Doctorat"
alias tlvac="t view context Vacations"
alias tlper="t view context Perso"
alias tllabo="t view context LCIS"

